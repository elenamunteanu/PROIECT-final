@page
@model project.Pages.Angajati.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h2>Angajați</h2>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Angajat[0].Prenume)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Angajat[0].Nume)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Angajat[0].Adresa)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Angajat[0].Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Angajat[0].Telefon)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Angajat) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Prenume)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nume)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Adresa)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Telefon)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.ID">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.ID">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.ID">Delete</a>
            </td>
        </tr>
}
        <audio autoplay>
            <source src="~/tarara.mp3" type="audio/mpeg" />
        </audio>
    </tbody>
</table>
